<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:colorPrimary">#334B82</item>
        <item name="android:colorPrimaryDark">#25365E</item>

    </style>
    <color name="white">#fff</color>
    <color name="black">#e6191919</color>
    <color name="mainHeader">#3B5796</color>
    <color name="mainHeaderDark">#25365E</color>
    <color name="academics">#014d41</color>
    <color name="academicsDark">#013629</color>
    <color name="hostel">#c2255a</color>
    <color name="hostelDark">#a92555</color>
    <color name="college">#e5ff7143</color>
    <color name="collegeDark">#e17142</color>
    <color name="stud">#6a1b9a</color>
    <color name="studDark">#5e147f</color>
    <color name="lifehack">#4a148c</color>
    <color name="lifehackDark">#31127f</color>
    <color name="around">#3f51b5</color>
    <color name="aroundDark">#344182</color>

    <array name="colors">
        <item>#1abc9c</item>
        <item>#2ecc71</item>
        <item>#3498db</item>
        <item>#9b59b6</item>
        <item>#f39c12</item>
        <item>#34495e</item>
        <item>#c0392b</item>
    </array>


    <style name="suggestionFont" parent="Base.TextAppearance.AppCompat.Small">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#FFF</item>
    </style>

    <array name="main_navigator_colors">
        <item>@color/academics</item>
        <item>@color/college</item>
        <item>@color/hostel</item>
        <item>@color/stud</item>
        <item>@color/lifehack</item>
        <item>@color/around</item>
    </array>


</resources>
